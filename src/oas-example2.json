        "Product": {
          "title": "Product",
          "type": "object",
          "properties": {
            "released": {
              "type": "string",
              "format": "date",
              "example": "2017-04-13",
              "nullable": true,
              "maximum": "2018-12-31",
              "minimum": "2018-01-01"
            }
          }
        }
      }
    }
  }